<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>
    let str ='abc';//string
    //查看内容的数据类型 typeof
    document.writeln(typeof str);
    str = 123;//number
    document.writeln(typeof str);
    const PI = 3.14;
    console.log(typeof PI);
    // PI = 1.2634;
    // console.log(PI);    
    //NaN = not a number
    let num1 = NaN;
    console.log(typeof num1);//NaN不是数字但是属于number类型
    let num2;
    console.log(typeof num2);//undefinded
    
    
</script>
</head>
